<!DOCTYPE html>
<html>
	<head>
		<title>Marathonrunnerphotos</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= javascript_include_tag "jquery_ujs" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
<div id="mainPan">
<div id="topPan"> <a href="http://0.0.0.0:3000/"><%= image_tag("logo.png") %></a>
	<ul>
		<li><%= link_to('Home') %></li>
		<li><%= link_to('Photos') %></li>
		<li><%= link_to('Friends') %></li>
		<li><%= link_to('Event') %></li>
    	<% if user_signed_in? %>
		Hello <%= current_user.first_name %>
			<span><%= link_to('Sign Out', destroy_user_session_path, :method => 'delete') %>
		<% else %>	
			<span><%= link_to('Sign In', new_user_session_path) %>
		<% end %>
    </ul>
</div>

		<%= yield %>

</div>
